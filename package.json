{
  "name": "zeecom",
  "private": true,
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev",
    "clean": "git clean -xdf node_modules",
    "clean:workspaces": "turbo run clean",
    "lint": "turbo lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "prettier": "3.3.3",
    "turbo": "^2.2.3",
    "typescript": "5.4.5"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "bun@1.1.33",
  "workspaces": [
    "apps/*",
    "packages/*",
    "tooling/*"
  ]
}
